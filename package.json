{
  "name": "font-to-swift",
  "version": "0.1.2",
  "keywords": [
    "swift",
    "font",
    "ios"
  ],
  "description": "Generate embeddable Swift code from a font file.",
  "main": "bin/font-to-swift.js",
  "bin": {
    "fontToSwift": "bin/font-to-swift.js"
  },
  "scripts": {
    "start": "node bin/font-to-swift.js",
    "testMulti": "npm start './test/fonts/'",
    "test": "npm start './test/fonts/GothamHTF-Book.otf'"
  },
  "preferGlobal": "true",
  "author": "Anas Halbawi",
  "license": "MIT",
  "dependencies": {
    "minimist": "^1.2.0",
    "opentype.js": "^0.6.6",
    "promise": "^7.1.1",
    "request": "^2.76.0",
    "rework": "^1.0.1",
    "unzip2": "^0.2.5"
  }
}
